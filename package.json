{
  "name": "my-vite-template",
  "version": "1.1.9",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "pnpm:check": "NPM_CHECK_INSTALLER=pnpm npm-check -u",
    "type:check": "vue-tsc --noEmit ",
    "lint:check": "eslint \"**/*.{vue,ts,js}\"",
    "lint:fix": "eslint \"**/*.{vue,ts,js}\" --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "serve": "vite preview"
  },
  "dependencies": {
    "@vueuse/core": "9.6.0",
    "pinia": "2.0.26",
    "vue": "3.2.45",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.31.0",
    "@pinia/testing": "0.0.14",
    "@vitejs/plugin-vue": "3.2.0",
    "@vitest/coverage-c8": "^0.25.3",
    "@vitest/ui": "0.25.3",
    "@vue/test-utils": "2.2.4",
    "autoprefixer": "10.4.13",
    "c8": "7.12.0",
    "eslint": "8.28.0",
    "happy-dom": "7.7.0",
    "http-server": "14.1.1",
    "msw": "0.49.0",
    "npm-check": "6.0.1",
    "postcss": "8.4.19",
    "sass": "1.56.1",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.3",
    "unplugin-auto-import": "0.11.5",
    "unplugin-vue-components": "0.22.11",
    "vite": "3.2.4",
    "vite-plugin-pages": "0.27.1",
    "vite-plugin-windicss": "1.8.8",
    "vitest": "0.25.3",
    "vue-tsc": "1.0.9",
    "windicss": "3.5.6"
  },
  "msw": {
    "workerDirectory": "public"
  }
}