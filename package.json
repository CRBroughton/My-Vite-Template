{
  "name": "my-vite-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "npm:check": "npx npm-check -u",
    "type:check": "vue-tsc --noEmit ",
    "lint:check": "eslint \"**/*.{vue,ts,js}\"",
    "lint:fix": "eslint \"**/*.{vue,ts,js}\" --fix",
    "serve": "vite preview",
    "test:unit": "npx cypress run-ct",
    "test:unit:web": "npx cypress open-ct",
    "test:e2e": "cypress run",
    "test:e2e:web": "cypress open",
    "test:report": "npx nyc report",
    "test:report:web": "http-server coverage/lcov-report -p 8000"
  },
  "dependencies": {
    "pinia": "^2.0.6",
    "vue": "^3.2.23",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.12.0",
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/vite-dev-server": "^2.2.1",
    "@cypress/vue": "^3.0.5",
    "@pinia/testing": "^0.0.8",
    "@vitejs/plugin-vue": "^1.10.1",
    "@vue/test-utils": "^2.0.0-rc.16",
    "cypress": "^9.1.1",
    "eslint": "^8.4.0",
    "http-server": "^14.0.0",
    "npm-check": "^5.9.2",
    "sass": "^1.44.0",
    "typescript": "^4.5.2",
    "unplugin-auto-import": "^0.5.1",
    "unplugin-vue-components": "^0.17.4",
    "vite": "^2.6.14",
    "vite-plugin-istanbul": "^2.3.0",
    "vite-plugin-pages": "^0.19.0-beta.6",
    "vite-plugin-windicss": "^1.5.4",
    "vue-tsc": "^0.29.8",
    "windicss": "^3.2.1"
  }
}
