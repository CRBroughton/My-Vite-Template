{
  "name": "my-vite-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "pnpm:check": "NPM_CHECK_INSTALLER=pnpm npm-check -u",
    "type:check": "vue-tsc --noEmit ",
    "lint:check": "eslint \"**/*.{vue,ts,js}\"",
    "lint:fix": "eslint \"**/*.{vue,ts,js}\" --fix",
    "serve": "vite preview"
  },
  "dependencies": {
    "@vueuse/core": "8.3.1",
    "pinia": "2.0.13",
    "vue": "3.2.33",
    "vue-router": "4.0.14"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.21.1",
    "@pinia/testing": "0.0.11",
    "@vitejs/plugin-vue": "2.3.1",
    "@vue/test-utils": "^2.0.0-rc.16",
    "eslint": "8.14.0",
    "http-server": "14.1.0",
    "msw": "0.39.2",
    "npm-check": "^5.9.2",
    "sass": "1.51.0",
    "typescript": "4.6.3",
    "unplugin-auto-import": "0.7.1",
    "unplugin-vue-components": "0.19.3",
    "vite": "2.9.6",
    "vite-plugin-pages": "0.23.0",
    "vite-plugin-windicss": "1.8.4",
    "vue-tsc": "0.34.10",
    "windicss": "3.5.1"
  },
  "msw": {
    "workerDirectory": "public"
  }
}