{
  "name": "my-vite-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "pnpm:check": "NPM_CHECK_INSTALLER=pnpm npm-check -u",
    "type:check": "vue-tsc --noEmit ",
    "lint:check": "eslint \"**/*.{vue,ts,js}\"",
    "lint:fix": "eslint \"**/*.{vue,ts,js}\" --fix",
    "serve": "vite preview",
    "test:unit": "npx cypress run-ct",
    "test:unit:web": "npx cypress open-ct",
    "test:e2e": "cypress run",
    "test:e2e:web": "cypress open",
    "test:report": "npx nyc report",
    "test:report:web": "http-server coverage/lcov-report -p 8000"
  },
  "dependencies": {
    "@vueuse/core": "7.6.1",
    "pinia": "2.0.11",
    "vue": "3.2.30",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.16.1",
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/vite-dev-server": "2.2.2",
    "@cypress/vue": "3.1.0",
    "@pinia/testing": "0.0.9",
    "@vitejs/plugin-vue": "2.2.0",
    "@vue/test-utils": "^2.0.0-rc.16",
    "cypress": "9.3.1",
    "eslint": "8.8.0",
    "http-server": "14.1.0",
    "npm-check": "^5.9.2",
    "sass": "1.49.7",
    "typescript": "4.5.5",
    "unplugin-auto-import": "0.5.11",
    "unplugin-vue-components": "0.17.18",
    "vite": "2.8.0",
    "vite-plugin-istanbul": "2.5.0",
    "vite-plugin-pages": "0.20.2",
    "vite-plugin-windicss": "1.7.0",
    "vue-tsc": "0.31.2",
    "windicss": "3.4.3"
  }
}
